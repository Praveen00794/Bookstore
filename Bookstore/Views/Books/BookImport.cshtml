
@{
    ViewBag.Title = "BookImport";
}

<h2>BookImport</h2>

<div>
    <button class="btn btn-success" type="button">
        <a href="../Bulkbookupdtemplate/BookExcelTemplate.xlsx">Download Template</a>
    </button>

    <div class="form-group row">
        <label class="col-md-12 col-form-label"> Browse File </label>
        <div class="col-md-12 form-group">
            <input type="file" class="form-control" id="flBookInfo" placeholder="Select File" />
        </div>
        <div class="form-group row">
            <div class="col-md-12 offset-0">
                <button class="btn btn-primary" id="btnBookImportID" type="button">SUBMIT</button>
                <button class="btn btn-primary" id="btnBookImportShow" type="button">SHOW</button>
                <button class="btn btn-primary" id="btnBookImportCLEAR" type="button">CLEAR</button>
            </div>
        </div>
    </div>

    <div class="col-md-12 form-group row" style="overflow:auto;">
        <table id="tblImportExcelData" class="table table-striped "></table>
    </div>
</div>